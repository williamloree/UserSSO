{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true ,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  }
}